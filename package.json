{
  "name": "@wiki3ai/lite-kernel",
  "version": "0.1.0",
  "description": "Custom kernel for Wiki3AI JupyterLite",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "static/**/*.{css,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "./labextension",
    "sharedPackages": {
      "@jupyterlab/application": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^4.4.5"
      },
      "@jupyterlab/apputils": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^4.4.5"
      },
      "@jupyterlab/services": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^7.4.5"
      },
      "@jupyterlab/settingregistry": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^4.4.5"
      },
      "@jupyterlab/translation": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^4.4.5"
      },
      "@jupyterlab/ui-components": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^4.4.5"
      },
      "@jupyterlite/kernel": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^0.6.4"
      },
      "@lumino/widgets": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^2.7.2"
      },
      "react": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^18.2.0"
      },
      "react-dom": {
        "bundled": false,
        "singleton": true,
        "requiredVersion": "^18.2.0"
      }
    }
  },
  "scripts": {
    "build": "tsc && jupyter labextension build .",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@jupyterlab/application": "4.4.5",
    "@jupyterlab/apputils": "4.4.5",
    "@jupyterlab/builder": "^4.4.10",
    "@jupyterlab/services": "7.4.5",
    "@jupyterlab/settingregistry": "4.4.5",
    "@jupyterlab/translation": "4.4.5",
    "@jupyterlab/ui-components": "4.4.5",
    "@jupyterlite/kernel": "0.6.4",
    "@lumino/widgets": "2.7.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@built-in-ai/web-llm": "^0.3.1",
    "@mlc-ai/web-llm": "^0.2.48",
    "ai": "^5.0.101"
  },
  "peerDependencies": {
    "@jupyterlab/application": "^4.4.5",
    "@jupyterlab/apputils": "^4.4.5",
    "@jupyterlab/services": "^7.4.5",
    "@jupyterlab/settingregistry": "^4.4.5",
    "@jupyterlab/translation": "^4.4.5",
    "@jupyterlab/ui-components": "^4.4.5",
    "@jupyterlite/kernel": "^0.6.4",
    "@lumino/widgets": "^2.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
